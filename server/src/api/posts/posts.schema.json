{
	"$schema": "http://json-schema.org/draft-07/schema#", 
	"title": "Post", 
	"type": "object",
	"required": [
		"title",
		"body",
		"user_id"
	],
	"properties": {
		"id": {
			"$id": "#root/id", 
			"title": "Id", 
			"type": "integer",
			"examples": [
				1
			]
		},
		"title": {
			"$id": "#root/title", 
			"title": "Title", 
			"type": "string",
			"examples": [
				"sunt aut facere repellat provident occaecati excepturi optio reprehenderit"
			],
			"pattern": "^.*$"
		},
		"body": {
			"$id": "#root/body", 
			"title": "Body", 
			"type": "string",
			"examples": [
				"quia et suscipit\nsuscipit recusandae consequuntur expedita et cum\nreprehenderit molestiae ut ut quas totam\nnostrum rerum est autem sunt rem eveniet architecto"
			],
			"pattern": "^.*$"
		},
		"user_id": {
			"$id": "#root/user_id", 
			"title": "User_id", 
			"type": "integer",
			"examples": [
				1
			],
			"default": 0
		},
		"created_at": {
			"$id": "#root/created_at", 
			"title": "Created_at", 
			"type": "string",
			"examples": [
				"2021-04-27T19:43:58.134Z"
			],
			"pattern": "^.*$"
		},
		"updated_at": {
			"$id": "#root/updated_at", 
			"title": "Updated_at", 
			"type": "string",
			"examples": [
				"2021-04-27T19:43:58.134Z"
			],
			"pattern": "^.*$"
		}
	}
}
